{
  "name": "conzeford",
  "version": "1.5.1",
  "main": "build/index.js",
  "repository": "https://github.com/UltimateDoge5/minecraft-panel.git",
  "author": "UltimateDoge5 <piotr5kozak@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "chalk": "4.1.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "node-gzip": "^1.1.2",
    "node-pty": "^0.10.1",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.9",
    "@types/lodash": "^4.14.184",
    "@types/node-gzip": "^1.1.0",
    "@types/ws": "^8.5.3",
    "cors": "^2.8.5"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && yarn prebuildClient && pkg build/index.js -c package.json --compress Brotli",
    "prebuildClient": "cd web && yarn build && yarn export"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "outputPath": "dist",
    "assets": [
      "web/out/**/*",
      "node_modules/bcrypt/lib/**/*",
      "node_modules/node-pty/build/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-linux-x64"
    ]
  }
}